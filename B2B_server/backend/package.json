{
  "name": "b2b-server-backend",
  "version": "1.0.0",
  "description": "B2B Server Backend API built with Express and TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha -r ts-node/register test/ResumeEvaluation.unit.test.ts --timeout 10000",
    "test:integration": "mocha -r ts-node/register test/ResumeEvaluation.integration.test.ts --timeout 60000",
    "test:all": "mocha -r ts-node/register test/**/*.test.ts --timeout 60000"
  },
  "keywords": [
    "b2b",
    "express",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.907.0",
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "@google/generative-ai": "^0.24.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.61.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.8.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^2.16.105",
    "pg": "^8.13.0",
    "rate-limiter-flexible": "^8.0.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.3.16",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.19.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.11.10",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "chai": "^4.4.1",
    "eslint": "^9.12.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.11",
    "prettier": "^3.3.3",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
